{

    "manifest_version": 2,
    "name": "The Box",
    "version": "1.0",
  
    "description": "A plugin that glitches a whole website into a single square box.",
  
    "icons": {
      "48": "icons/malevich_blacksquare.jpg"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.mozilla.org/*"],
        "js": ["thebox.js"]
      }
    ]
  
  }