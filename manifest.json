
{

  "description": "A plugin that glitches a whole website into a single square box.",
  "manifest_version": 2,
  "name": "black square",
  "version": "1.0",

  "permissions": [
    "activeTab"
  ],

  "icons": {
    "48": "icons/malevich_black_square_48.jpg"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_icon": {
      "16": "icons/malevich_black_square_16.jpg",
      "32": "icons/malevich_black_square_32.jpg"
    }
  }

}